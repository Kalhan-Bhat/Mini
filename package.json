{
  "name": "student-engagement-portal",
  "version": "2.0.0",
  "description": "Real-time video-based virtual classroom with AI-powered emotion detection",
  "scripts": {
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend-node && npm install",
    "setup:frontend": "cd frontend-react && npm install",
    "dev:backend": "cd backend-node && npm start",
    "dev:frontend": "cd frontend-react && npm run dev",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "education",
    "emotion-detection",
    "video-call",
    "react",
    "nodejs",
    "fastapi",
    "pytorch",
    "agora"
  ],
  "author": "",
  "license": "MIT"
}
